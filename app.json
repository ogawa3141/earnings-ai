{
  "expo": {
    "name": "Earnings AI",
    "slug": "earnings-ai",
    "scheme": "earnings-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#131A2A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.earningsai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#131A2A"
      },
      "package": "com.earningsai.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": ["expo-router"]
  }
}
